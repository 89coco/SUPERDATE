<div class="container">
  <h3>Create a new super</h3>
      <%= simple_form_for @super do |f| %>
        <%= f.input :super_type, placeholder: 'Villain' %>
        <%= f.input :universe %>
        <%= f.input :price %>
        <%= f.input :availability %>
        <%= f.input :super_name %>
        <%= f.input :photos, as: :file, input_html: { multiple: true } %>
        <%= f.submit class: 'btn btn-flat' %>
      <% end %>
</div>
